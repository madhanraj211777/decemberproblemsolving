import java.util.*;
public class Main 
{
	public static void main(String[] args) {
	    Scanner scan=new Scanner(System.in);
	    int number=scan.nextInt();
	    int palindrome=number;
	    int rev=0;
	    int rem=0;
	    while(number!=0){
	    rem=number%10;
	    rev=rev*10+rem;
	    number=number/10;
	    }
	    
	    if(palindrome==rev){
	        System.out.println("Yes,it is palindrome");
	    }else{
	        System.out.println("No,it is no a palindrome");
	    }
	    
	
	}
}
